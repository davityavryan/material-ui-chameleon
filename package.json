{
    "name": "material-ui-chameleon",
    "version": "0.1.0-alpha.2",
    "description": "Material-UI Chameleon - Theme editor for Material-UI.",
    "author": "Davit Yavryan <davit.yavryan@gmail.com>",
    "license": "MIT",
    "scripts": {
        "build": "yarn build:umd && yarn build:cjs && yarn build:esm && yarn build:es && yarn build:copy-files",
        "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ./babel.config.js ./src --out-dir ./build --ignore \"**/*.test.js\"",
        "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ./babel.config.js ./src --out-dir ./build/esm --ignore \"**/*.test.js\"",
        "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./babel.config.js ./src --out-dir ./build/es --ignore \"**/*.test.js\"",
        "build:umd": "cross-env BABEL_ENV=production-umd rollup -c ./rollup.config.js",
        "build:copy-files": "node ./copy-files.js",
        "demo:start": "webpack-dev-server --progress --mode development",
        "demo:build": "webpack-cli --progress -p",
        "demo:production": "webpack-dev-server --progress -p",
        "demo:deploy": "yarn demo:build && gh-pages -d ./gh-pages",
        "predemo:start": "yarn install --prefer-offline --frozen-lockfile",
        "predemo:build": "yarn install --prefer-offline --frozen-lockfile",
        "predemo:production": "yarn install --prefer-offline --frozen-lockfile",
        "prebuild": "yarn install --prefer-offline --frozen-lockfile && rimraf build",
        "prepublishOnly": "yarn build"
    },
    "sideEffects": false,
    "dependencies": {
        "object-path": "^0.11.4",
        "react-color": "^2.17.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.0",
        "@babel/core": "^7.7.2",
        "@babel/plugin-proposal-class-properties": "^7.7.0",
        "@babel/plugin-proposal-export-default-from": "^7.5.2",
        "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
        "@babel/plugin-transform-object-assign": "^7.2.0",
        "@babel/plugin-transform-react-constant-elements": "^7.6.3",
        "@babel/plugin-transform-runtime": "^7.6.2",
        "@babel/preset-env": "^7.7.1",
        "@babel/preset-react": "^7.7.0",
        "@babel/register": "^7.7.0",
        "@material-ui/core": "^4.6.1",
        "@material-ui/icons": "^4.5.1",
        "@rollup/plugin-replace": "^2.2.1",
        "babel-loader": "^8.0.6",
        "babel-plugin-optimize-clsx": "^2.5.0",
        "babel-plugin-react-remove-properties": "^0.3.0",
        "babel-plugin-transform-dev-warning": "^0.1.1",
        "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.0.5",
        "core-js": "^3.4.1",
        "cross-env": "^6.0.3",
        "fs-extra": "^8.1.0",
        "gh-pages": "^2.1.1",
        "glob": "^7.1.6",
        "html-webpack-plugin": "^3.2.0",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "rimraf": "^3.0.0",
        "rollup": "^1.27.2",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-terser": "^5.1.2",
        "script-ext-html-webpack-plugin": "^2.1.4",
        "terser-webpack-plugin": "^2.2.1",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.9.0"
    },
    "peerDependencies": {
        "@material-ui/core": "^4.5.0",
        "react": "^16.8.0",
        "react-dom": "^16.8.0"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "keywords": [
        "react",
        "react-component",
        "material design",
        "material-ui",
        "theme editor"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/davityavryan/material-ui-chameleon.git"
    },
    "bugs": {
        "url": "https://github.com/davityavryan/material-ui-chameleon/issues"
    },
    "homepage": "https://davityavryan.github.io/material-ui-chameleon"
}
