{
    "name": "material-ui-chameleon",
    "version": "0.1.0-alpha.6",
    "description": "Material-UI Chameleon - Theme editor for Material-UI.",
    "author": "Davit Yavryan <davit.yavryan@gmail.com>",
    "license": "MIT",
    "private": true,
    "sideEffects": false,
    "scripts": {
        "build": "yarn build:umd && yarn build:cjs && yarn build:esm && yarn build:es && yarn build:copy-files",
        "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ./babel.config.js ./src --out-dir ./build --ignore \"**/*.test.js\"",
        "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ./babel.config.js ./src --out-dir ./build/esm --ignore \"**/*.test.js\"",
        "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./babel.config.js ./src --out-dir ./build/es --ignore \"**/*.test.js\"",
        "build:umd": "cross-env BABEL_ENV=production-umd rollup -c ./rollup.config.js",
        "build:copy-files": "node ./copy-files.js",
        "demo:start": "yarn --cwd ./gh-pages start",
        "demo:build": "yarn --cwd ./gh-pages build",
        "demo:production": "yarn --cwd ./gh-pages production",
        "demo:deploy": "yarn --cwd ./gh-pages deploy",
        "cy:run": "BABEL_ENV=test cypress run -q --headless --record false",
        "cy:open": "BABEL_ENV=test cypress open",
        "test": "BABEL_ENV=test start-server-and-test demo:production 4040 cy:run",
        "test:ci": "BABEL_ENV=test start-server-and-test 'yarn demo:production --progress=0 --watch=0' 4040 'yarn cy:run --record'",
        "test:open": "BABEL_ENV=test start-server-and-test demo:production 4040 cy:open",
        "prebuild": "yarn install --prefer-offline --frozen-lockfile && rimraf build",
        "precy:run": "yarn install --prefer-offline --frozen-lockfile && rimraf .nyc_output coverage",
        "precy:open": "yarn install --prefer-offline --frozen-lockfile && rimraf .nyc_output coverage",
        "pretest": "yarn install --prefer-offline --frozen-lockfile",
        "pretest:open": "yarn install --prefer-offline --frozen-lockfile",
        "prepublishOnly": "yarn build"
    },
    "dependencies": {
        "deepmerge": "^4.2.2",
        "object-path": "^0.11.5",
        "react-color": "^2.18.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.11.6",
        "@babel/core": "^7.11.6",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/plugin-proposal-export-default-from": "^7.10.4",
        "@babel/plugin-transform-object-assign": "^7.10.4",
        "@babel/plugin-transform-react-constant-elements": "^7.10.4",
        "@babel/plugin-transform-runtime": "^7.11.5",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-react": "^7.10.4",
        "@cypress/code-coverage": "^3.8.1",
        "@material-ui/core": "^4.9.10",
        "@material-ui/icons": "^4.9.1",
        "@rollup/plugin-commonjs": "^15.1.0",
        "@rollup/plugin-node-resolve": "^11.0.0",
        "@rollup/plugin-replace": "^2.3.3",
        "babel-plugin-istanbul": "^6.0.0",
        "babel-plugin-optimize-clsx": "^2.6.1",
        "babel-plugin-react-remove-properties": "^0.3.0",
        "babel-plugin-transform-dev-warning": "^0.1.1",
        "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
        "cross-env": "^7.0.2",
        "cypress": "^5.3.0",
        "fs-extra": "^9.0.1",
        "glob": "^7.1.6",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "rimraf": "^3.0.2",
        "rollup": "^2.29.0",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-terser": "^7.0.2",
        "start-server-and-test": "^1.11.5"
    },
    "peerDependencies": {
        "@material-ui/core": "^4.9.10",
        "react": "^16.8.0",
        "react-dom": "^16.8.0"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "keywords": [
        "react",
        "react-component",
        "material design",
        "material-ui",
        "theme editor"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/davityavryan/material-ui-chameleon.git"
    },
    "bugs": {
        "url": "https://github.com/davityavryan/material-ui-chameleon/issues"
    },
    "homepage": "https://davityavryan.github.io/material-ui-chameleon"
}
